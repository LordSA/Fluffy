{
  "name": "fluffy-bot",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:lavalink && npm run start:bot",
    "start:lavalink": "pm2 start java --name 'Lavalink' --cwd './Lavalink' -- -jar Lavalink.jar",
    "start:bot": "pm2 start index.js --name 'fluffy' --watch --ignore-watch='node_modules logs'",
    "stop": "pm2 delete fluffy && pm2 delete Lavalink",
    "restart": "pm2 restart fluffy && pm2 restart Lavalink",
    "logs": "pm2 logs fluffy"
  },
  "dependencies": {
    "@discord-player/extractor": "^4.4.0",
    "@discordjs/voice": "^0.16.0",
    "@distube/spotify": "^2.0.0",
    "@distube/yt-dlp": "^2.0.0",
    "@google/generative-ai": "^0.21.0",
    "colors": "^1.4.0",
    "discord-player": "^6.6.0",
    "discord.js": "^14.14.0",
    "distube": "^5.0.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.2.0",
    "jsoning": "^0.11.22",
    "libsodium-wrappers": "^0.7.13",
    "opusscript": "^0.0.8",
    "pretty-ms": "^7.0.1",
    "shoukaku": "^4.1.0",
    "simple-git": "^3.24.0",
    "socket.io": "^4.7.4",
    "sodium-native": "^4.1.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  }
}